#include <Servo.h>

Servo m0;
Servo m1;
Servo m2;
Servo m3;
Servo m4;
Servo m5;
Servo m6;
Servo m7;

void setup() {
  m0.attach(4);
  m1.attach(5);
  m2.attach(6);
  m3.attach(7);
  m4.attach(8);
  m5.attach(9);
  m6.attach(10);
  m7.attach(11);
}

void loop() {
  m0.write(90);
  m1.write(90);
  m2.write(90);
  m3.write(90);
  m4.write(90);
  m5.write(90);
  m6.write(90);
  m7.write(90);
}

























































